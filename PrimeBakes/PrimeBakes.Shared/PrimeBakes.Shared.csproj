    <PackageReference Include="Syncfusion.Blazor.Popups" Version="32.2.4" />
    <PackageReference Include="Syncfusion.Blazor.Themes" Version="32.2.4" />
    <PackageReference Include="Toolbelt.Blazor.HotKeys2" Version="6.2.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\PrimeBakesLibrary\PrimeBakesLibrary.csproj" />
  </ItemGroup>

  <ItemGroup>
